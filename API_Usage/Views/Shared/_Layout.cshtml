<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - Stock Appplication</title>
  <link rel="stylesheet" href="~/css/site.css" />
  @*<script type="text/javascript" src="~/js/Chart.min.js"></script>*@
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
  <script type="text/javascript" src="~/lib/chartjs-plugin-annotation-master/chartjs-plugin-annotation.min.js"></script>
  <script type="text/javascript" src="~/js/site.js"></script>
</head>
<body>
  <nav>
      <div class="nav-bar">
          <a asp-area="" asp-controller="Home" asp-action="Index">Home </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Symbols">Symbols </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Chart">Chart </a><text> | </text>
          <a asp-area="" asp-controller="Logo" asp-action="CompanyDetail">  Company Information </a><text> | </text>
          <a asp-area="" asp-controller="Trade" asp-action="Trade"> Largest Trades </a><text> | </text>
          <a asp-area="" asp-controller="EffectiveSpread" asp-action="EffectiveSpread"> Effective Spread </a><text> | </text>
          <a asp-area="" asp-controller="Dividend" asp-action="Dividend"> Dividends </a><text> | </text>
          <a asp-area="" asp-controller="Watchlists" asp-action="Index"> My Watchlist </a><text> | </text>
          <a asp-area="" asp-controller="Highlights" asp-action="News"> Latest Highlights </a><text> | </text>
          <a asp-area="" asp-controller="Home" asp-action="Refresh">Refresh </a><text> | </text>
          <a asp-area="" asp-controller="About" asp-action="AboutUs">  About </a><text> | </text>

      </div>
  </nav>
  <div class="container body-content">
    @RenderBody()
    <hr />
    <footer>
        <p>Copyright &copy; 2019 USF ISM6225 Group2. All Rights Reserved.</p>
    </footer>
  </div>
  @RenderSection("Scripts", required: false)
</body>
</html>